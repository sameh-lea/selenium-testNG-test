import org.openqa.selenium.support.How;

import com.e2eTests.automatedTests.utils.CommonMethods;

public class alertPageObjects {
	CommonMethods common = new CommonMethods();

	/* locator */
	final static String BOUTTON_ID = "alertButton";

	/* FindBy */
	@FindBy(how = How.ID, using = BOUTTON_ID)
	public static WebElement boutton;

	/* Method */
	public void clickBoutton() {
		boutton.click();
	}
	
	public void acceptPopUP {
		
 }
	public void getTitle() {
		
	}
}
